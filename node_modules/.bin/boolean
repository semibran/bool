#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/brandon/Desktop/dev/boolean/bin/index.js/node_modules:/Users/brandon/Desktop/dev/boolean/bin/node_modules:/Users/brandon/Desktop/dev/boolean/node_modules:/Users/brandon/Desktop/dev/node_modules:/Users/brandon/Desktop/node_modules:/Users/brandon/node_modules:/Users/node_modules:/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/index.js" "$@"
else 
  exec node  "$basedir/../../bin/index.js" "$@"
fi
